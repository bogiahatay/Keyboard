<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="1.5%p">
    <Row>
        <Key
            android:codes="49"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyLabel="6" />
        <Key
            android:codes="55"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyLabel="9" />
        <Key
            android:codes="48"
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>

    <Row>
        <Key
            android:codes="113"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyLabel="q"
            android:popupCharacters="~"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key

            android:codes="119"
            android:isModifier="true"
            android:keyLabel="w"
            android:popupCharacters="`"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="101"
            android:keyLabel="e"
            android:popupCharacters="|"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="114"
            android:keyLabel="r"
            android:popupCharacters="•"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="116"
            android:keyLabel="t"
            android:popupCharacters="√"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="121"
            android:keyLabel="y"
            android:popupCharacters="π"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="117"
            android:keyLabel="u"
            android:popupCharacters="÷"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="105"
            android:keyLabel="i"
            android:popupCharacters="×"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="111"
            android:keyLabel="o"
            android:popupCharacters="¶"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="112"
            android:keyEdgeFlags="right"
            android:keyLabel="p"
            android:popupCharacters="∆"
            android:popupKeyboard="@xml/keyboard_popup_template" />
    </Row>

    <Row>
        <Key
            android:codes="97"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="a"
            android:popupCharacters="\@"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="115"
            android:keyLabel="s"
            android:popupCharacters="#"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="100"
            android:keyLabel="d"
            android:popupCharacters="$"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="102"
            android:keyLabel="f"
            android:popupCharacters="%"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="103"
            android:keyLabel="g"
            android:popupCharacters="&amp;"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="104"
            android:keyLabel="h"
            android:popupCharacters="-"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="106"
            android:keyLabel="j"
            android:popupCharacters="+"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="107"
            android:keyLabel="k"
            android:popupCharacters="("
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="108"
            android:keyEdgeFlags="right"
            android:keyLabel="l"
            android:popupCharacters=")"
            android:popupKeyboard="@xml/keyboard_popup_template" />
    </Row>

    <Row>
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift_ios8"
            android:keyWidth="14%p" />
        <Key
            android:codes="122"
            android:horizontalGap="1%p"
            android:keyLabel="z"
            android:popupCharacters="*"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="120"
            android:keyLabel="x"
            android:popupCharacters="&quot;"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="99"
            android:keyLabel="c"
            android:popupCharacters="'"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="118"
            android:keyLabel="v"
            android:popupCharacters=":"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="98"
            android:keyLabel="b"
            android:popupCharacters=";"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="110"
            android:keyLabel="n"
            android:popupCharacters="!"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="109"
            android:keyLabel="m"
            android:popupCharacters="\?"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key

            android:codes="-5"
            android:horizontalGap="1%p"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete_ios8"
            android:keyWidth="14%p" />
    </Row>

    <Row android:rowEdgeFlags="bottom">

        <Key
            android:codes="-2"
            android:keyLabel="\?123"
            android:keyWidth="15%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />

        <Key
            android:codes="-101"
            android:keyIcon="@drawable/sym_keyboard_language_switch"
            android:keyWidth="0%p" />
        <Key
            android:codes="32"
            android:keyLabel="@string/language_vi"
            android:keyWidth="50%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupCharacters=".,?!#@"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return_ios8"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>
    