<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularTextView">
        <attr name="ctv_fill_color" format="color" />
        <attr name="ctv_border_color" format="color" />
        <attr name="ctv_border_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GodImageView">
        <attr name="gtv_type">
            <enum name="square" value="1" />
            <enum name="sixteen_nine" value="2" />
            <enum name="four_three" value="3" />
        </attr>
        <attr name="gtv_border_color" format="color" />
        <attr name="gtv_border_width" format="dimension" />
        <attr name="gtv_top_crop" format="boolean" />
    </declare-styleable>
</resources>